From 02977f15dbb0ffc55ae28b3f52d955cd7c63a84e Mon Sep 17 00:00:00 2001
From: Michael Webster <miketwebster@gmail.com>
Date: Sun, 14 Dec 2025 08:45:03 -0500
Subject: [PATCH] ExtensionCore.py: GtkLabel argument should be a string.

Fixes #13259
---
 files/usr/share/cinnamon/cinnamon-settings/bin/ExtensionCore.py | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/files/usr/share/cinnamon/cinnamon-settings/bin/ExtensionCore.py b/files/usr/share/cinnamon/cinnamon-settings/bin/ExtensionCore.py
index d493c0c7dc..56a4fc93c9 100644
--- files/usr/share/cinnamon/cinnamon-settings/bin/ExtensionCore.py
+++ files/usr/share/cinnamon/cinnamon-settings/bin/ExtensionCore.py
@@ -767,7 +767,7 @@ def __init__(self, uuid, data, spices, size_groups):
         score_box = Gtk.Box()
         score_image = Gtk.Image.new_from_icon_name('xsi-starred-symbolic', 2)
         score_box.pack_start(score_image, False, False, 0)
-        score_label = Gtk.Label(self.score)
+        score_label = Gtk.Label(label=str(self.score))
         score_box.pack_start(score_label, False, False, 5)
         widget.pack_start(score_box, False, False, 0)
         size_groups[1].add_widget(score_box)
